{
    "name": "s-koval/hasoffers",
    "description": "A HasOffers Client for Rulerr",
    "keywords": ["api", "Rulerr", "Rulerr-HasOffers", "HasOffers", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "rulerr/rulerr": "dev-master"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Rulerr\\HasOffers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rulerr\\Tests\\HasOffers\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
